#: $string['template']
msgid ""
"<table class=\"custombox-theorema\" cellspacing=\"0\" width=\"100%\">\n"
"<tr valign=\"top\">\n"
"    <td class=\"custombox-header-thumb theorema\" style=\"background-image : "
"url(<%%icon%%>);\" width=\"2%\" rowspan=\"3\">\n"
"    </td>\n"
"    <td class=\"custombox-header-caption theorema\" width=\"98%\">\n"
"        Theorema\n"
"    </td>\n"
"</tr>\n"
"<tr valign=\"top\">\n"
"    <td class=\"custombox-content theorema\">\n"
"        <%%theorema%%>\n"
"    </td>\n"
"</tr>\n"
"<tr valign=\"top\">\n"
"    <td class=\"custombox-corollaries theorema\">\n"
"        <%%corollarylist%%>\n"
"    </td>\n"
"</tr>\n"
"<%if %%showdemonstration%% %>\n"
"<tr valign=\"top\">\n"
"    <td class=\"custombox-header-foo theorema\" width=\"2%\"></td>\n"
"    <td class=\"custombox-demonstration theorema\">\n"
"        <div class=\"custombox-demonstration-caption "
"theorema\">Demonstration</div>\n"
"        <div class=\"custombox-demonstration theorema\"><%%demonstration%%"
"></div>\n"
"    </td>\n"
"</tr>\n"
"<%endif %>\n"
"</table>"
