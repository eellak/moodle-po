#: $string['template']
msgid ""
"<table class=\"custombox-worktodo\" cellspacing=\"0\" width=\"100%\">\n"
"<tr valign=\"top\">\n"
"    <td class=\"custombox-header-thumb worktodo\" style=\"background-image : "
"url(<%%icon%%>);\" width=\"2%\" rowspan=\"3\">\n"
"    </td>\n"
"    <td class=\"custombox-header-caption worktodo\" width=\"98%\" colspan=\"2\">\n"
"        Work to do\n"
"    </td>\n"
"</tr>\n"
"<tr valign=\"top\">\n"
"    <td class=\"custombox-worktype worktodo\" colspan=\"2\">\n"
"        <span class=\"custombox-param worktodo\">Nature: </span> <span class"
"=\"custombox-value worktodo\"><%%worktypefield%%></span>\n"
"    </td>\n"
"    <td class=\"custombox-timeexpected worktodo\" align=\"right\" width=\"40\">\n"
"        <img src=\"<%%clock%%>\" /> <%%estimatedworktime%%>\n"
"    </td>\n"
"</tr>\n"
"<tr valign=\"top\">\n"
"    <td class=\"custombox-worktype worktodo\">\n"
"        <span class=\"custombox-param worktodo\">Effort: </span> <span class"
"=\"custombox-value worktodo\"><%%workeffortfield%%></span>\n"
"    </td>\n"
"    <td class=\"custombox-workmode worktodo\" align=\"right\" colspan=\"3\">\n"
"        <span class=\"custombox-param worktodo\">Mode: </span> <span class"
"=\"custombox-value worktodo\"><%%workmodefield%%></span>\n"
"    </td>\n"
"</tr>\n"
"<tr>\n"
"    <td class=\"custombox-content worktodo\" colspan=\"2\">\n"
"        <%%worktodo%%>\n"
"    </td>\n"
"</tr>\n"
"</table>"
msgstr ""

