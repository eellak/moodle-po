#: $string['calc_int_methods_parts_fact']
msgid ""
"\\[\n"
"\\int_a^b u{{\\rm d}v\\over {\\rm d}x}{\\rm d}x=\\left[uv\\right]_a^b-\n"
"\\int_a^b{{\\rm d}u\\over {\\rm d}x}v\\,{\\rm d}x\\]\n"
"or alternatively: \\[\\int_a^bf(x)g(x)\\,{\\rm d}x=\\left[f(x)\\,\\int\n"
"g(x){\\rm d}x\\right]_a^b -\\int_a^b{{\\rm d}f\\over {\\rm\n"
"d}x}\\left\\{\\int g(x){\\rm d}x\\right\\}{\\rm d}x.\\]"
msgstr ""

#: $string['calc_int_methods_parts_name']
msgid "Integration by Parts"
msgstr ""

#: $string['calc_int_methods_substitution_fact']
msgid ""
"\\[\n"
"\\int f(u){{\\rm d}u\\over {\\rm d}x}{\\rm d}x=\\int f(u){\\rm d}u\n"
"\\quad\\hbox{and}\\quad \\int_a^bf(u){{\\rm d}u\\over {\\rm d}x}\\,{\\rm\n"
"d}x = \\int_{u(a)}^{u(b)}f(u){\\rm d}u.\n"
"\\]"
msgstr ""

#: $string['calc_int_methods_substitution_name']
msgid "Integration by Substitution"
msgstr ""

#: $string['calc_int_standard_integrals_fact']
msgid ""
"\\[\\int k\\ dx = kx +c, \\mbox{ where k is constant.}\\]\n"
"\\[\\int x^n\\ dx  = \\frac{x^{n+1}}{n+1}+c, \\quad (n\\ne -1)\\]\n"
"\\[\\int x^{-1}\\ dx = \\int {\\frac{1}{x}}\\ dx = \\ln(|x|)+c = \\ln(k*|x|) = "
"\\left\\{\\matrix{\\ln(x)+c & x>0\\cr\n"
"\\ln(-x)+c & x<0\\cr}\\right.\\]\n"
"\n"
"<center>\n"
"<table>\n"
"<tr><th>\\(f(x)\\)</th><th> \\(\\int f(x)\\ dx\\)</th></tr>\n"
"<tr><td>\\(e^x\\) </td> <td>  \\(e^x+c\\)</td> <td> </td> </tr>\n"
"<tr><td>\\(\\cos(x)\\) </td> <td>  \\(\\sin(x)+c\\)   </td> <td> </td> </tr>\n"
"<tr><td>\\(\\sin(x)\\) </td> <td>  \\(-\\cos(x)+c\\)  </td> <td> </td> </tr>\n"
"<tr><td>\\(\\tan(x)\\) </td> <td>  \\(\\ln(\\sec(x))+c\\) </td> "
"<td>\\(-\\frac{\\pi}{2} < x < \\frac{\\pi}{2}\\)</td> </tr>\n"
"<tr><td>\\(\\sec x\\)  </td> <td>  \\(\\ln (\\sec(x)+\\tan(x))+c\\) </td> <td> \\( "
"-{\\pi\\over 2}< x < {\\pi\\over 2}\\)</td> </tr>\n"
"<tr><td>cosec\\(\\, x\\) </td> <td>  \\(\\ln ($cosec$(x)-\\cot(x))+c\\) </td> "
"<td>\\(0 < x < \\pi\\)</td> </tr>\n"
"<tr><td>cot\\(\\,x\\) </td> <td>  \\(\\ln(\\sin(x))+c\\) </td> <td>  \\(0< x< \\pi\\) "
"</td> </tr>\n"
"<tr><td>\\(\\cosh(x)\\) </td> <td>  \\(\\sinh(x)+c\\)</td> <td></td> </tr>\n"
"<tr><td>\\(\\sinh(x)\\) </td> <td>  \\(\\cosh(x) + c\\) </td> <td> </td> </tr>\n"
"<tr><td>\\(\\tanh(x)\\) </td> <td>  \\(\\ln(\\cosh(x))+c\\)</td> <td> </td> </tr>\n"
"<tr><td>coth\\((x)\\) </td> <td>  \\(\\ln(\\sinh(x))+c \\)</td> <td>   "
"\\(x>0\\)</td> </tr>\n"
"<tr><td>\\({1\\over x^2+a^2}\\) </td> <td>  \\({1\\over a}\\tan^{-1}{x\\over "
"a}+c\\)</td> <td> \\(a>0\\)</td> </tr>\n"
"<tr><td>\\({1\\over x^2-a^2}\\) </td> <td>  \\({1\\over 2a}\\ln{x-a\\over x+a}+c\\) "
"</td> <td>  \\(|x|>a>0\\)</td> </tr>\n"
"<tr><td>\\({1\\over a^2-x^2}\\) </td> <td>  \\({1\\over 2a}\\ln{a+x\\over a-x}+c\\) "
"</td> <td>   \\(|x|<a\\)</td> </tr>\n"
"<tr><td>\\({1\\over \\sqrt{x^2+a^2}}\\) </td> <td>  "
"\\(\\sinh^{-1}\\left(\\frac{x}{a}\\right) + c\\) </td> <td> \\(a>0\\) </td> </tr>\n"
"<tr><td>\\({1\\over \\sqrt{x^2-a^2}}\\) </td> <td>  "
"\\(\\cosh^{-1}\\left(\\frac{x}{a}\\right) + c\\) </td> <td>  \\(x\\geq a > 0\\) </td> "
"</tr>\n"
"<tr><td>\\({1\\over \\sqrt{x^2+k}}\\) </td> <td>  \\(\\ln "
"(x+\\sqrt{x^2+k})+c\\)</td> <td> </td> </tr>\n"
"<tr><td>\\({1\\over \\sqrt{a^2-x^2}}\\) </td> <td>  "
"\\(\\sin^{-1}\\left(\\frac{x}{a}\\right)+c\\)</td> <td>  \\(-a\\leq x\\leq a\\)  </td> "
"</tr>\n"
"</table></center>"
msgstr ""

