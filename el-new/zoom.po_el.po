msgid "Sessions"
msgstr "Συνεδρίες"

#: $string['start']
msgid "Start"
msgstr "Έναρξη"

#: $string['starthostjoins']
msgid "Start video when host joins"
msgstr "Έναρξη βίντεο όταν εισέλθει ο παρουσιαστής"

#: $string['start_meeting']
msgid "Start Meeting"
msgstr "Έναρξη Σύσκεψης"

#: $string['startpartjoins']
msgid "Start video when participant joins"
msgstr "Έναρξη βίντεο όταν εισέλθουν οι συμμετέχοντες"

