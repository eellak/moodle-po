msgstr ""

#: $string['pluginname']
msgid "IP address list quiz access rule"
msgstr ""

#: $string['subnet']
msgid "IP subnet"
msgstr ""

#: $string['subnet_help']
msgid ""
"Specify comma-separated list of partial or full IP address numbers.\n"
"\n"
"Examples:\n"
"\n"
"* 192.168.10.1\n"
"* 192.168.\n"
"* 231.54.211.0/20\n"
"* 231.3.56.10-20\n"
"* 192.168.10.1,192.168.,231.54.211.0/20,231.3.56.10-20"
msgstr ""

#: $string['subnetwrong']
msgid ""
"This quiz is only accessible from certain locations, and this computer is "
"not on the allowed list."
msgstr ""
